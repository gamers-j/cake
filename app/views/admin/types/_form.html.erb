<div class="row mt-5">
    <div class="row form-group">
      <%= form_with model: [:admin, type] do |f| %>
      <div class="d-flex flex-row bd-highlight ml-6">
        <div class="form-group ">
          <%= f.label :name,"ジャンル名" %>
        </div>
        <div class="form-group ml-5">
          <div class="col-sm-12 ">
          <%= f.text_field :name, placeholder: "ジャンル名", class: "form-control text_field" %>
          </div>
        </div>
        <div class="form-group ml-5">
          <%= f.submit "新規登録", class: 'btn btn-success' %>
        </div>
      </div>
      <% end %>
    </div>
  </div>